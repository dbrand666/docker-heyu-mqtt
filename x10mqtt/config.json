{
  "name": "X10 MQTT Gateway",
  "version": "dev",
  "slug": "x10mqtt",
  "description": "MQTT gateway for the X10 CM11",
  "url": "https://github.com/mmotley999/x10mqtt",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "auto_uart": true,
  "map": ["config:rw", "ssl"],
  "options": {
    "serial_port": "/dev/ttyUSB0"
    "mqtt_host": "",
    "mqtt_port": 1833,
    "mqtt_user": ""
	"mqtt_pass": ""
  },
  "schema": {
    "serial_port": "str",
    "mqtt_host": "str",
	"mqtt_port": "int",
    "mqtt_user": "str?",
	"mqtt_pass": "str?"
  }
}
